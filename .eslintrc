{
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "^_"
            }
        ]
    }
}